import("//flutter/build/dart/rules.gni")
import("//flutter/testing/run_tests.gni")

tests = [ "fail_test.dart" ]

foreach(test, tests) {
  flutter_dart_test(test) {
    test_name = test
  }
}

group("smoke_test_failure") {
  testonly = true
  deps = []
  foreach(test, tests) {
    deps += [ ":compile_$test" ]
  }
}

group("_run_tests_py_smoke_test_failure") {
  testonly = true
  deps = []
  foreach(test, tests) {
    deps += [ ":_run_tests_py_${test}" ]
  }
}
